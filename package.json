{
  "name": "hello-cordova",
  "version": "0.0.1",
  "author": {
    "name": "Kiki le singe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiki-le-singe/hello-cordova.git"
  },
  "homepage": "https://github.com/kiki-le-singe/hello-cordova",
  "bugs": "https://github.com/kiki-le-singe/hello-cordova/issues",
  "keywords": [
    "cordova",
    "react",
    "react-router",
    "redux",
    "framework7",
    "font-awesome",
    "es6",
    "ES2015",
    "babeljs",
    "webpack",
    "express"
  ],
  "description": "This is a boilerplate for starting a project with ReactJS... Webpack Dev Server is used to serve the application in dev mode. Express is used for the api",
  "engines": {
    "node": ">= 0.12.0"
  },
  "files": [
    "CHANGELOG.md",
    "README.md"
  ],
  "dependencies": {
    "body-parser": "^1.14.0",
    "classnames": "^2.2.0",
    "express": "^4.13.0",
    "font-awesome": "^4.3.0",
    "framework7": "^1.3.1",
    "history": "^1.12.6",
    "jquery": "^2.1.4",
    "lodash": "^3.10.0",
    "mongodb": "^2.0.46",
    "mongoose": "^4.2.2",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "superagent": "^1.4.0",
    "uniqid": "^1.0.0",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.29",
    "browser-sync": "^2.9.6",
    "clean-webpack-plugin": "^0.1.4",
    "css-loader": "^0.21.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.9.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-iconify": "^0.3.2",
    "gulp-load-plugins": "^1.0.0",
    "html-webpack-plugin": "^1.6.1",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.3",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-safari-launcher": "^0.1.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "require-dir": "^0.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "scripts": {
    "cordova:create": "npm run cordova:platform:add && npm run cordova:plugin:add",
    "cordova:platform:add": "cordova platform add ios android",
    "cordova:plugin:add": "cordova plugin add cordova-plugin-device cordova-plugin-network-information",
    "dev:cordova": "NODE_ENV=dev_cordova webpack --config webpack.cordova.dev.config.js && cordova run android",
    "deploy:cordova": "NODE_ENV=prod_cordova webpack -p --progress --config webpack.cordova.production.config.js && cordova run android",
    "deploy": "NODE_ENV=production webpack -p --progress --config webpack.production.config.js",
    "dev": "npm run build | node server/app.js",
    "dev:debug": "npm run dev -- --debug",
    "dev:quiet": "npm run dev -- --quiet",
    "dev:stub": "npm run dev -- --stub",
    "build": "webpack"
  }
}
